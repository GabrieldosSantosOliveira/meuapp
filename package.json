{
  "name": "meuapp",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "tsnd -r tsconfig-paths/register --respawn --transpile-only ./src/main/server",
    "start": "node ./dist/main/server",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier -w .",
    "build": "rimraf ./dist && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "test:integration": "npm run test -- -c jest-integration-config.ts --watch",
    "test:unit": "npm run test -- -c jest-unit-config.ts --watch",
    "test": "jest --passWithNoTests",
    "test:staged": "jest --passWithNoTests",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GabrieldosSantosOliveira/meuapp.git"
  },
  "keywords": [],
  "author": "Gabriel dos Santos Oliveira <gabrielsantosoliveira951@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GabrieldosSantosOliveira/meuapp/issues"
  },
  "homepage": "https://github.com/GabrieldosSantosOliveira/meuapp#readme",
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.5",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.14",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.6",
